{
	"extends": [
		"tslint-react",
		"tslint:recommended"
	],
	"rules": {
		"quotemark": false,
		"indent": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"member-ordering": false,
		"no-console": false,
		"jsx-no-multiline-js": false,
		"no-unused-expression": false,
		"no-empty-interface": false,
		"member-access": false
	}
}